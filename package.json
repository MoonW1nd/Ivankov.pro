{
  "name": "Ivankov.pro",
  "private": true,
  "description": "Personal site Alexander Ivankov",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint:ts": "./node_modules/.bin/tslint ./src/**/*.+\\(ts\\|tsx\\)",
    "lint:style": "./node_modules/.bin/stylelint ./src/**/*.+\\(sass\\|scss\\|css\\|less\\)",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.config.js'"
    }
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "antd": "^3.23.6",
    "classnames": "^2.2.6",
    "commitizen": "^4.0.4",
    "gatsby": "^2.18.10",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-less": "^3.0.9",
    "gatsby-plugin-ts": "^2.0.1",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-remark": "^2.6.27",
    "less": "^3.10.3",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "snyk": "^1.278.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/ramda": "^0.26.26",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "babel-plugin-import": "^1.12.2",
    "css-property-sort-order-smacss": "^2.1.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "prettier": "1.18.2",
    "stylelint": "11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "^2.2.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.11.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:ts --fix",
      "git add"
    ],
    "*.{sass,scss,css,less}": [
      "npm run lint:style --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoonW1nd/Ivankov.pro.git"
  },
  "keywords": [
    "site",
    "personal",
    "site"
  ],
  "author": "Alexander Ivankov",
  "bugs": {
    "url": "https://github.com/MoonW1nd/Ivankov.pro/issues"
  },
  "homepage": "https://github.com/MoonW1nd/Ivankov.pro#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
