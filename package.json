{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "Personal site Alexander Ivankov",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint:ts": "./node_modules/.bin/tslint ./src/**/*.+\\(ts\\|tsx\\)",
    "lint:style": "./node_modules/.bin/stylelint ./src/**/*.+\\(sass\\|scss\\|css\\)",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.config.js'"
    }
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.6",
    "antd": "^3.23.6",
    "commitizen": "^4.0.3",
    "gatsby": "^2.15.29",
    "gatsby-plugin-antd": "^2.0.2",
    "gatsby-plugin-less": "^3.0.9",
    "gatsby-plugin-ts-loader": "^0.1.0",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-remark": "^2.6.27",
    "less": "^3.10.3",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@types/ramda": "^0.26.26",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "babel-plugin-import": "^1.12.2",
    "css-property-sort-order-smacss": "^2.1.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "stylelint": "11.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "^2.2.1",
    "stylelint-scss": "^3.11.1",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.6.3"
  },
  "lint-staged": {
    "*.[ts, tsx]": [
      "npm run lint:ts --fix",
      "git add"
    ],
    "*.{sass, scss, css}": [
      "npm run lint:style --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoonW1nd/Ivankov.pro.git"
  },
  "keywords": [
    "site",
    "personal",
    "site"
  ],
  "author": "Alexander Ivankov",
  "bugs": {
    "url": "https://github.com/MoonW1nd/Ivankov.pro/issues"
  },
  "homepage": "https://github.com/MoonW1nd/Ivankov.pro#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
